Class {
	#name : #DvtBase64ImageStorage,
	#superclass : #DvtSingleton,
	#instVars : [
		'base64Images'
	],
	#category : #'DataVisualizationTool-Control'
}

{ #category : #accessing }
DvtBase64ImageStorage class >> globalName [
	^ #DvtImages
]

